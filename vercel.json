{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "routes": [
    {
      "src": "/api/addStudents(.*)",
      "dest": "/src/controllers/addStudents.controllers.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/generateCertificates(.*)",
      "dest": "/src/controllers/certificate.controller.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/sendEmails(.*)",
      "dest": "/src/controllers/sendEmail.controllers.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/user/login(.*)",
      "dest": "/src/controllers/user.controller.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/user/updatepassword(.*)",
      "dest": "/src/controllers/user.controller.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/user/updateemail(.*)",
      "dest": "/src/controllers/user.controller.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/user/sendOtp(.*)",
      "dest": "/src/controllers/user.controller.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/user/adminpassword(.*)",
      "dest": "/src/controllers/user.controller.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/courses/add(.*)",
      "dest": "/src/controllers/courses.controllers.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/courses/getAll(.*)",
      "dest": "/src/controllers/courses.controllers.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/api/courses/delete/:course_id(.*)",
      "dest": "/src/controllers/courses.controllers.js",
      "methods": [
        "DELETE"
      ]
    }
  ],
  "functions": {
    "/api/addStudents(.*)": {
      "src": "/src/controllers/addStudents.controllers.js",
      "memory": 512,
      "maxDuration": 30
    },
    "/api/generateCertificates(.*)": {
      "src": "/src/controllers/certificate.controller.js",
      "memory": 512,
      "maxDuration": 30
    },
    "/api/sendEmails(.*)": {
      "src": "/src/controllers/sendEmail.controllers.js",
      "memory": 512,
      "maxDuration": 30
    },
    "/api/user/login(.*)": {
      "src": "/src/controllers/user.controller.js",
      "memory": 256,
      "maxDuration": 20
    },
    "/api/user/updatepassword(.*)": {
      "src": "/src/controllers/user.controller.js",
      "memory": 256,
      "maxDuration": 20
    },
    "/api/user/updateemail(.*)": {
      "src": "/src/controllers/user.controller.js",
      "memory": 256,
      "maxDuration": 20
    },
    "/api/user/sendOtp(.*)": {
      "src": "/src/controllers/user.controller.js",
      "memory": 256,
      "maxDuration": 20
    },
    "/api/user/adminpassword(.*)": {
      "src": "/src/controllers/user.controller.js",
      "memory": 256,
      "maxDuration": 20
    },
    "/api/courses/add(.*)": {
      "src": "/src/controllers/courses.controllers.js",
      "memory": 256,
      "maxDuration": 20
    },
    "/api/courses/getAll(.*)": {
      "src": "/src/controllers/courses.controllers.js",
      "memory": 256,
      "maxDuration": 20
    },
    "/api/courses/delete/:course_id(.*)": {
      "src": "/src/controllers/courses.controllers.js",
      "memory": 256,
      "maxDuration": 20
    }
  }
}